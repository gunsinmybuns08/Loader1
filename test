-- Scripts:

local function XXXC_fake_script() -- cbro.LocalScript 
	local script = Instance.new('LocalScript', cbro)

	function Leftclick()
	
	if game.PlaceId == 301549746 then
		script.Parent.Parent.Parent.Main.Visible = false
		wait(0.9)
		script.Parent.Parent.Parent.Pic.Visible = true
		wait(0.9)
		script.Parent.Parent.Parent.Pic.Visible = false
		script.Parent.Parent.Parent.Loading1.Visible = true
		wait(0.9)
		script.Parent.Parent.Parent.Loading1.Visible = false
		script.Parent.Parent.Parent.Loading2.Visible = true
		wait(0.9)
		script.Parent.Parent.Parent.Loading2.Visible = false
		script.Parent.Parent.Parent.Loading3.Visible = true
		wait(0.9)
		script.Parent.Parent.Parent.Loading3.Visible = false
		script.Parent.Parent.Parent.Loading1.Visible = true
		wait(0.9)
		script.Parent.Parent.Parent.Loading1.Visible = false
		script.Parent.Parent.Parent.Loading2.Visible = true
		wait(0.9)
		script.Parent.Parent.Parent.Loading2.Visible = false
		script.Parent.Parent.Parent.Loading3.Visible = true
		wait(0.9)
		script.Parent.Parent.Parent.Loading3.Visible = false
		wait(0.9)
	
	
	else
			game.Players.LocalPlayer:kick("Game not supported Games supported:\n counterblox, phantomforces")
		end
	end
	
	script.Parent.MouseButton1Click:Connect(Leftclick)
end
coroutine.wrap(XXXC_fake_script)()
local function NCHYG_fake_script() -- Loader.Drag 
	local script = Instance.new('LocalScript', Loader)

	script.Parent.Main.Active = true
	script.Parent.Main.Selectable = true
	script.Parent.Main.Draggable = true
	script.Parent.Login.Active = true
	script.Parent.Login.Selectable = true
	script.Parent.Login.Draggable = true
end
coroutine.wrap(NCHYG_fake_script)()
local function LAIM_fake_script() -- Enter.LocalScript 
	local script = Instance.new('LocalScript', Enter)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.Loginbox.Text == "nn" then
			script.Parent.Parent.Parent.Login.Visible = false
			wait(0.5)
			script.Parent.Parent.Parent.Loginloading.Visible = true
			wait(1)
			script.Parent.Parent.Parent.Loginloading.Visible = false
			wait(0.4)
			script.Parent.Parent.Parent.Main.Visible = true
		else	
			game.Players.LocalPlayer:kick("Wrong key please look at discord for key")
		end
		
	end)
end
coroutine.wrap(LAIM_fake_script)()
local function HAJFTJO_fake_script() -- Loader.Key system 
	local script = Instance.new('Script', Loader)

	if Key == ("nn") then
		script.Parent.Login.Visible = true
	else
		game.Players.LocalPlayer:kick("Wrong key")
	end
end
coroutine.wrap(HAJFTJO_fake_script)()
